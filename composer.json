{
    "name": "vitexsoftware/abraflexi-webhook-acceptor",
    "description": "AbraFlexi WebHook acceptor",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "ext-posix": "*",
        "vitexsoftware/ease-fluentpdo": "^1.4",
        "vitexsoftware/ease-bootstrap5-widgets-abraflexi": "dev-main",
        "vitexsoftware/ease-bootstrap5-widgets": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpstan/phpstan": "*",
        "friendsofphp/php-cs-fixer": "^3.87",
        "ergebnis/composer-normalize": "^2.48",
        "ergebnis/php-cs-fixer-config": "^6.57",
        "phpstan/phpstan-phpunit": "2.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "AbraFlexi\\Acceptor\\": "src/AbraFlexi/Acceptor/",
            "AbraFlexi\\Acceptor\\Saver\\": "src/AbraFlexi/Acceptor/Saver/",
            "AbraFlexi\\Acceptor\\Ui\\": "src/AbraFlexi/Acceptor/Ui/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
