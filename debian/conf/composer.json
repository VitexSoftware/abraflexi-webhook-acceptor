{
  "name": "vitexsoftware/abraflexi-webhook-acceptor",
  "description": "Tool used to setup AbraFlexi multiinstance",
  "version": "0.6",
  "type": "project",
  "require": {
    "deb/abraflexi": "*",
    "deb/ease-bricks": "*",
    "deb/abraflexi-bricks": "*",
    "deb/ease-fluentpdo": "*"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Vítězslav Dvořák",
      "email": "info@vitexsoftware.cz"
    }
  ],
  "config": {
    "vendor-dir": "/var/lib/abraflexi-webhook-acceptor"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "AbraFlexi\\Acceptor\\": "./Acceptor/",
      "AbraFlexi\\Acceptor\\Saver\\": "./Acceptor/Saver/",
      "AbraFlexi\\Acceptor\\Ui\\": "./Acceptor/Ui/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "/usr/share/php/EaseCore/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseHtml/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseBricks/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseTWB4/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/AbraFlexi/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/AbraFlexiBricks/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseFluentPDO/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseTWB4Widgets/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseTWB4WidgetsAbraFlexi/"
    }
  ]
}
